{
  "name": "filepond-plugin-tarball",
  "version": "1.0.0",
  "description": "Tarball FilePond Directory Uploads",
  "source": "src/tarball.ts",
  "main": "dist/tarball.js",
  "module": "dist/tarball.esm.js",
  "unpkg": "dist/tarball.min.js",
  "scripts": {
    "dev": "microbundle watch -i src/tarball.ts -o ./bundled/tarball.js --name FilePondPluginTarball --no-sourcemap --no-pkg-main --no-generateTypes --format umd --no-compress",
    "build": "npm-run-all --parallel build:dist build:bundle",
    "build:dist": "microbundle build --name FilePondPluginTarball",
    "build:bundle": "npm-run-all build:bundle-full build:bundle-min",
    "build:bundle-full": "microbundle build -i src/tarball.ts -o ./bundled/tarball.js --name FilePondPluginTarball --no-sourcemap --no-pkg-main --no-generateTypes --format umd --no-compress",
    "build:bundle-min": "microbundle build -i src/tarball.ts -o ./bundled/tarball.min.js --name FilePondPluginTarball --no-sourcemap --no-pkg-main --no-generateTypes --format umd",
    "lint": "eslint src",
    "test": "jest --verbose",
    "test:ci": "jest --ci --runInBand --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtaku3/filepond-plugin-tarball.git"
  },
  "keywords": [
    "filepond",
    "tarball directory",
    "upload directory"
  ],
  "author": {
    "name": "mtaku3",
    "email": "m.taku3.1222@gmail.com",
    "url": "https://mtaku3.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mtaku3/filepond-plugin-tarball/issues"
  },
  "homepage": "https://github.com/mtaku3/filepond-plugin-tarball#readme",
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@gera2ld/tarjs": "^0.2.2",
    "@obsidize/tar-browserify": "^3.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-jest": "^29.0.1",
    "eslint": "^7.12.1",
    "eslint-config-tc": "^16.1.0",
    "eslint-config-typescript-tc": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unicorn": "^22.0.0",
    "filepond": "^4.21.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "microbundle": "^0.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "tarts": "^2.0.1",
    "typescript": "^5.0.4"
  }
}